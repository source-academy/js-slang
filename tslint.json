{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "cyclomatic-complexity": {
      "severity": "warning",
      "options": 20
    },
    "semicolon": ["never"],
    "quotemark": ["single"],
    "trailing-comma": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning"
    },
    "arrow-parens": false
  },
  "rulesDirectory": []
}
