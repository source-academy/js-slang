{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "semicolon": ["never"],
    "quotemark": ["single"],
    "trailing-comma": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "only-arrow-functions": false,
    "triple-equals": false,
    "radix": false,
    "no-empty": false,
    "no-console": {
      "severity": "warning"
    },
    "whitespace": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
